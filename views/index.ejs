<!DOCTYPE html>
<html>
  <head>
    <title>Home</title>
    <link rel="stylesheet" href="/styles.css">
  </head>
  <body>
    <%- include('navbar', {links: links}) %>
    <h1>The Board of Messages</h1>
    <div class="board">
        <ul>
            <% messages.forEach((message) => { %>
            <%- include('messages', {message: message}); %>
            <% }); %>
        </ul>
    </div>
    <script>console.log(messages)</script>
  </body>
</html>